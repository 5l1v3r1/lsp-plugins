<plugin width="700" height="128" resizable="false">
	<hbox size="6" border="0">
		<align pad_left="4" pad_right="4">
			<vbox>
				<label text="Bypass" />
				<switch id="bypass" angle="1" />
				<led id="bypass" color="red" size="8" />
				<label />
			</vbox>
		</align>
		
		<vbox>
			<hbox>
				<vbox>
					<align expand="true" pad_bottom="4">
						<graph width="640" height="340" expand="true" fill="true">
							<center hpos="-1" vpos="-1" visible="false" />
							
							<!-- Gray grid -->
							<ui:for id="f" first="20" last="90" step="10"><marker value="${f}" color="gray" basis="0" parallel="1" /></ui:for>
							<ui:for id="f" first="200" last="900" step="100"><marker value="${f}" color="gray" basis="0" parallel="1" /></ui:for>
							<ui:for id="f" first="2000" last="9000" step="1000"><marker value="${f}" color="gray" basis="0" parallel="1" /></ui:for>
							<marker value="20000" color="gray" basis="0" parallel="1" />
							
							<marker value="63.09575" color="gray" basis="1" parallel="0" /> <!-- +36 dB -->
							<marker value="3.98107" color="gray" basis="1" parallel="0" /> <!-- +12 dB -->
							<marker value="0.25119" color="gray" basis="1" parallel="0" /> <!-- -12 dB -->
							<marker value="0.01585" color="gray" basis="1" parallel="0" /> <!-- -36 dB -->
							
							<!-- Yellow grid -->
							<marker value="100" color="yellow" basis="0" parallel="1" /> <!-- 100 Hz -->
							<marker value="1000" color="yellow" basis="0" parallel="1" /> <!-- 1 kHz -->
							<marker value="10000" color="yellow" basis="0" parallel="1" /> <!-- 10 kHz -->
			
							<marker value="15.84893" color="yellow" basis="1" parallel="0" /> <!-- 24 dB -->
							<marker value="1" color="white" basis="1" parallel="0" /> <!-- 0 dB -->
							<marker value="0.06310" color="yellow" basis="1" parallel="0" /> <!-- -24 dB -->
			
							<!-- Meshes -->
							<ui:for id="i" first="0" last="15">
								<marker id="f_${i}" basis="0" parallel="1" color="eq_base" hue_id="hue_${i}" visibility_id="fv_${i}" />
								<marker id="g_${i}" basis="1" parallel="0" color="eq_base" hue_id="hue_${i}" visibility_id="fv_${i}" />
							</ui:for>
							<ui:for id="i" first="0" last="15">
								<mesh id="agf_${i}" color="eq_base" width="2" hue_id="hue_${i}" visibility_id="fv_${i}" fill="0.8" smooth="true" />
							</ui:for>
							<mesh id="fftg_l" width="2" color="left_channel" visibility_id="fftv_l" />
							<mesh id="fftg_r" width="2" color="right_channel" visibility_id="fftv_r" />
							<mesh id="ag" width="3" color="middle_channel" smooth="true" />
							<ui:for id="i" first="0" last="15">
								<dot hpos_id="f_${i}" vpos_id="g_${i}" scroll_id="q_${i}"
									editable="true" color="eq_base"
									hue_id="hue_${i}" visibility_id="fv_${i}"
								/>
							</ui:for>
							
							<!-- Boundaries -->
							<marker value="251.18861" color="white" basis="1" parallel="0" /> <!-- +48 dB -->
							<marker value="24000" color="white" basis="0" parallel="1" />
							
							<!-- Axis -->
							<axis min="10" max="24000" color="white" angle="0.0" logarithmic="true" />
							<axis min="0.00398" max="251.18861" color="white" angle="0.5" logarithmic="true" /> <!-- -48..+48 dB -->
							
							<!-- Text -->
							<text text="Hz" coord="10;0.00398" halign="1" valign="1" />
							<text text="100" coord="100;0.00398" halign="1" valign="1" />
							<text text="1K" coord="1000;0.00398" halign="1" valign="1" />
							<text text="10K" coord="10000;0.00398" halign="1" valign="1" />
							
							<text text="+36" coord="10;63.09575" halign="1" valign="-1" />
							<text text="+24" coord="10;15.84893" halign="1" valign="-1" />
							<text text="+12" coord="10;3.98107" halign="1" valign="-1" />
							<text text="0" coord="10;1" halign="1" valign="1" />
							<text text="-12" coord="10;0.25119" halign="1" valign="1" />
							<text text="-24" coord="10;0.06310" halign="1" valign="1" />
							<text text="-36" coord="10;0.01585" halign="1" valign="1" />
							<text text="dB" coord="10;251.18861" halign="1" valign="-1" />
						</graph>
					</align>
					<hbox spacing="8">
						<label text="Filters" />
						<combo id="fsel" />
						<label text="Mode" />
						<combo id="mode" />
						<align hpos="1" expand="true">
							<hbox spacing="8">
								<button id="fftv_l" size="16" color="left_channel" led="true" />
								<label text="Left" />
								<button id="fftv_r" size="16" color="right_channel" led="true" />
								<label text="Right" />
							</hbox>
						</align>
						<align pad_right="4" hpos="1"  expand="true"><label text="SPECTRUM GRAPH" /></align>
					</hbox>
				</vbox>
				<vbox>
					<group text="Signal" expand="true">
						<grid rows="6" cols="2" transpose="true">
							<label text="Input" />
							<knob id="g_in" expand="true" />
							<value id="g_in" />
							<label text="Output" expand="true" />
							<knob id="g_out" />
							<value id="g_out" />
							
							<label text="Balance" expand="true" />
							<knob id="bal" balance="0.5" scale_color="green" />
							
							<cell rows="4">
								<grid rows="3" cols="2">
									<meter id="sml" angle="1" type="vu" height="80" logarithmic="true" min="0.01585" max="2" color="left_channel" pad_left="2" pad_right="2" />
									<meter id="smr" angle="1" type="vu" height="80" logarithmic="true" min="0.01585" max="2" color="right_channel" pad_left="2" pad_right="2" />
									<label text="L" color="left_channel" font_size="8" />
									<label text="R" color="right_channel" font_size="8" />
									<cell cols="2"><value id="bal" font_size="8" same_line="true" /></cell>
								</grid>
							</cell>
						</grid>
					</group>
					<group text="Analysis" expand="true">
						<vbox>
							<label text="FFT" halign="0" />
							<combo id="fft" />
							
							<grid rows="3" cols="3" transpose="true" expand="true" >
								<label text="Reactivity" />
								<knob id="react" size="20" />
								<value id="react" />
								
								<label text="Shift" />
								<knob id="shift" size="20" />
								<value id="shift" />
							</grid>
						</vbox>
					</group>
				</vbox>
			</hbox>
			<hbox>
				<group text="Filters 0-7" expand="true" visibility_id="fsel" visibility_key="0">
					<grid transpose="true" rows="8" cols="17">
						<label text="#" fill="true" halign="0" />
						<label text="Filter" fill="true" halign="0" />
						<label text="Mode" fill="true" halign="0" />
						<label text="Slope" fill="true" halign="0" />
						<vbox fill="true" valign="0.5">
							<label text="Freq," halign="0" />
							<label text="Gain" halign="0" />
						</vbox>
						<label text="(Hz), (dB)" fill="true" halign="0" />
						<vbox fill="true">
							<label text="Q," halign="0" />
							<label text="Hue" halign="0" />
						</vbox>
						<label />
						
						<!-- Filters -->
						<ui:for id="i" first="0" last="7">
							<cell cols="2"><label text="${i}" /></cell>
							<cell cols="2"><combo id="ft_${i}" width="48" /></cell>
							<cell cols="2"><combo id="fm_${i}" width="48" /></cell>
							<combo id="s_${i}" width="8" />
							<knob id="f_${i}" scale_hue_id="hue_${i}" size="20" />
							<value id="f_${i}" detailed="false" precision="1" />
							<knob id="q_${i}" scale_hue_id="hue_${i}" size="20" />
							<value id="q_${i}" detailed="false" />
							<grid rows="2" cols="2">
								<button id="xs_${i}" led="true" size="12" color="green" />
								<button id="xm_${i}" led="true" size="12" color="red" />
								<label text="S" font_size="8" />
								<label text="M" font_size="8" />
							</grid>
							<knob id="g_${i}" scale_hue_id="hue_${i}" size="20" />
							<value id="g_${i}" detailed="false" />
							<knob id="hue_${i}" scale_hue_id="hue_${i}" size="16" />
							<value id="hue_${i}" detailed="false" />
						</ui:for>
					</grid>
				</group>
				<group text="Filters 8-15" expand="true" visibility_id="fsel" visibility_key="1">
					<grid transpose="true" rows="8" cols="17">
						<label text="#" fill="true" halign="0" />
						<label text="Filter" fill="true" halign="0" />
						<label text="Mode" fill="true" halign="0" />
						<label text="Slope" fill="true" halign="0" />
						<vbox fill="true" valign="0.5">
							<label text="Freq," halign="0" />
							<label text="Gain" halign="0" />
						</vbox>
						<label text="(Hz), (dB)" fill="true" halign="0" />
						<vbox fill="true">
							<label text="Q," halign="0" />
							<label text="Hue" halign="0" />
						</vbox>
						<label />
						
						<!-- Filters -->
						<ui:for id="i" first="8" last="15">
							<cell cols="2"><label text="${i}" /></cell>
							<cell cols="2"><combo id="ft_${i}" width="48" /></cell>
							<cell cols="2"><combo id="fm_${i}" width="48" /></cell>
							<combo id="s_${i}" width="8" />
							<knob id="f_${i}" scale_hue_id="hue_${i}" size="20" />
							<value id="f_${i}" detailed="false" precision="1" />
							<knob id="q_${i}" scale_hue_id="hue_${i}" size="20" />
							<value id="q_${i}" detailed="false" />
							<grid rows="2" cols="2">
								<button id="xs_${i}" led="true" size="12" color="green" />
								<button id="xm_${i}" led="true" size="12" color="red" />
								<label text="S" font_size="8" />
								<label text="M" font_size="8" />
							</grid>
							<knob id="g_${i}" scale_hue_id="hue_${i}" size="20" />
							<value id="g_${i}" detailed="false" />
							<knob id="hue_${i}" scale_hue_id="hue_${i}" size="16" />
							<value id="hue_${i}" detailed="false" />
						</ui:for>
					</grid>
				</group>
			</hbox>
		</vbox>
	</hbox>
</plugin>